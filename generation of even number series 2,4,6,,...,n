#include<stdio.h>
int main()
{
	int i,n;
	printf("print all even numbers till: ");
	scanf("%d",&n);
	printf("even numbers from 1 to %d are:\n",n);
	for(i=1;i<=n;i++)
	{
		if(i%2==0)
		{
			printf("%d\n",i);
		}
	}
	return 0;
}

OUTPUT:
print all even numbers till: 6
even numbers from 1 to 6 are:
2
4
6
