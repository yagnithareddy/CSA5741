#include<stdio.h>
int main()
{
	int n,sum=0,remainder;
	printf("enter an integer:");
	scanf("%d",&n);
	while(n>0)
	{
		remainder=n%10;
		sum=sum+remainder;
		n=n/10;
	}
	printf("\n sum of the digits of given number=%d",sum);
	return 0;
}


OUTPUT:
enter an integer:23
sum of the digits of given number=5
